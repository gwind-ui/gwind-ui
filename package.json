{
    "name": "gwindui",
    "version": "1.0.3-3",
    "description": "Gwind UI is a modern and customizable Vue 3 UI Component library designed to help developers build responsive and accessible user interfaces with ease. Built with the latest web technologies like Vue 3, TailwindCSS, and TypeScript, Gwind UI provides a seamless development experience with reusable, modular, and intuitive components.",
    "author": "Fatich Imam Al Arasy <ifatich@gmail.com>",
    "contributors": [
        {
            "name": "Fatich Imam Al Arasy",
            "email": "ifatich@gmail.com"
        },
        {
            "name": "Miftahul Ichsan"
        }
    ],
    "type": "module",
    "main": "./lib/index.js",
    "module": "./lib/index.js",
    "types": "./lib/index.d.ts",
    "exports": {
        ".": {
            "types": "./lib/index.d.ts",
            "import": "./lib/index.js",
            "require": "./lib/index.js"
        },
        "./lib/style.css": {
            "import": "./lib/gwind-ui.css",
            "require": "./lib/gwind-ui.css"
        }
    },
    "sideEffects": false,
    "files": [
        "lib",
        "src/components/",
        "package.json",
        "README.md"
    ],
    "scripts": {
        "run:dev": "vite",
        "run:preview": "vite preview",
        "run:demo": "pnpm --dir ./examples run dev",
        "build:lib": "vite build",
        "build:demo": "pnpm build:lib && pnpm --dir ./examples run build",
        "build:storybook": "storybook build",
        "danger:lint-report": "NODE_ENV=production eslint src --format checkstyle --output-file reports/lint-results.xml",
        "danger:ci": "danger ci",
        "prettier": "prettier --write . ",
        "lint": "eslint --fix",
        "prepare": "husky",
        "storybook": "storybook dev -p 6006",
        "run:changelog": "conventional-changelog -i CHANGELOG.md -s -r 0"
    },
    "dependencies": {
        "@internationalized/date": "^3.7.0",
        "@pegadaian/gwind": "1.0.2-4",
        "@pegadaian/gwind-ic": "0.0.11",
        "@tanstack/vue-table": "^8.21.2",
        "@vueuse/core": "^11.3.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "datatables.net": "^2.3.2",
        "datatables.net-dt": "^2.3.2",
        "datatables.net-responsive": "^3.0.4",
        "datatables.net-responsive-dt": "^3.0.4",
        "jquery": "^3.7.1",
        "lucide-vue-next": "^0.475.0",
        "ohash": "^2.0.11",
        "reka-ui": "^2.3.0",
        "tailwind-merge": "^3.0.2",
        "unplugin-vue-define-options": "^1.5.5",
        "vite-plugin-static-copy": "^2.3.0",
        "vue": "^3.5.13"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "^3.2.6",
        "@eslint/eslintrc": "^3.3.0",
        "@eslint/js": "^9.22.0",
        "@storybook/addon-docs": "^8.6.7",
        "@storybook/addon-essentials": "^8.6.7",
        "@storybook/addon-interactions": "^8.6.7",
        "@storybook/addon-onboarding": "^8.6.7",
        "@storybook/blocks": "^8.6.7",
        "@storybook/test": "^8.6.7",
        "@storybook/vue3": "^8.6.7",
        "@storybook/vue3-vite": "^8.6.7",
        "@tailwindcss/postcss": "^4.1.7",
        "@types/node": "^22.13.10",
        "@typescript-eslint/eslint-plugin": "^8.26.1",
        "@typescript-eslint/parser": "^8.26.1",
        "@vitejs/plugin-vue": "^5.2.3",
        "conventional-changelog-cli": "^5.0.0",
        "danger": "10.6.0",
        "danger-plugin-lint-report": "^1.8.1",
        "danger-plugin-pull-request": "^1.1.4",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-formatter-checkstyle": "^8.40.0",
        "eslint-import-resolver-alias": "^1.1.2",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.3",
        "eslint-plugin-storybook": "^0.11.6",
        "eslint-plugin-vue": "^9.33.0",
        "globals": "^15.15.0",
        "husky": "^9.1.7",
        "lint-staged": "^15.5.0",
        "postcss": "^8.5.3",
        "prettier": "^3.5.3",
        "sass": "^1.86.0",
        "sass-embedded": "^1.86.0",
        "storybook": "^8.6.7",
        "storybook-addon-vue-slots": "^0.9.29",
        "tailwindcss": "^4.1.7",
        "typescript": "~5.7.3",
        "typescript-eslint": "^8.26.1",
        "unplugin-auto-import": "^0.18.6",
        "unplugin-vue-components": "^0.27.5",
        "vite": "^6.2.2",
        "vite-plugin-dts": "^4.5.3",
        "vue-eslint-parser": "^9.4.3",
        "vue-tsc": "^2.2.8"
    },
    "peerDependencies": {
        "danger": " 10.6.0"
    }
}
