#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lord/Documents/asd/gwind-vue/node_modules/.pnpm/prettyjson@1.2.5/node_modules/prettyjson/bin/node_modules:/Users/lord/Documents/asd/gwind-vue/node_modules/.pnpm/prettyjson@1.2.5/node_modules/prettyjson/node_modules:/Users/lord/Documents/asd/gwind-vue/node_modules/.pnpm/prettyjson@1.2.5/node_modules:/Users/lord/Documents/asd/gwind-vue/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lord/Documents/asd/gwind-vue/node_modules/.pnpm/prettyjson@1.2.5/node_modules/prettyjson/bin/node_modules:/Users/lord/Documents/asd/gwind-vue/node_modules/.pnpm/prettyjson@1.2.5/node_modules/prettyjson/node_modules:/Users/lord/Documents/asd/gwind-vue/node_modules/.pnpm/prettyjson@1.2.5/node_modules:/Users/lord/Documents/asd/gwind-vue/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettyjson@1.2.5/node_modules/prettyjson/bin/prettyjson" "$@"
else
  exec node  "$basedir/../../../../../prettyjson@1.2.5/node_modules/prettyjson/bin/prettyjson" "$@"
fi
